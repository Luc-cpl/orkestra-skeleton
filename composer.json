{
    "name": "luccpl/orkestra-skeleton",
    "description": "My Orkestra application",
    "type": "project",
    "scripts": {
        "post-create-project-cmd": [
            "composer install --no-dev --no-interaction -q",
            "php maestro app:create --replace",
            "composer install"
        ]
    },
    "autoload": {
        "psr-4": {
            "Orkestra\\Skeleton\\": "installer/app"
        }
    },
    "version": "1.0.0",
    "require": {
        "luccpl/orkestra": "1.0.0"
    }
}
